{
	"name": "@directus/sdk-js",
	"version": "9.0.0-rc.43",
	"description": "The official Directus SDK for use in JavaScript!",
	"main": "node/index.js",
	"types": "types/index.d.ts",
	"files": [
		"dist",
		"types",
		"node",
		"browser"
	],
	"scripts": {
		"prebuild": "rimraf ./dist && rimraf ./types && rimraf ./node && rimraf ./browser",
		"build": "run-p build:*",
		"build:types": "tsc --project ./tsconfig.types.json",
		"build:node": "tsc --project ./tsconfig.node.json",
		"dev": "jest --coverage --watch",
		"test": "jest --coverage"
	},
	"keywords": [
		"api",
		"client",
		"cms",
		"directus",
		"headless",
		"javascript",
		"node",
		"sdk"
	],
	"author": "Rijk van Zanten <rijkvanzanten@me.com>",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.21.1"
	},
	"devDependencies": {
		"axios-mock-adapter": "^1.19.0",
		"jest": "^26.6.3",
		"jest-environment-jsdom": "^26.6.2",
		"jest-environment-jsdom-global": "^2.0.4",
		"jest-mock-axios": "^4.3.0",
		"npm-run-all": "^4.1.5",
		"openapi3-ts": "^2.0.0",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.5.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.2"
	}
}
